entity Virtualmachine extends std::Dataclass:
    string name
    string os
    int ram
    int cpus
    int disk
end

implement Virtualmachine using std::none

two = Virtualmachine(name="A", os="B", ram=1, cpus=2, disk=10)

one=make_virtual_machine()

one.requires = one

std::print(one)

eat_vm(one)
eat_vm(two)

